        "prefix": "bm-collapse",
        "body": [
            "<bm-collapse v-model=\"${1:activeName}\" @change=\"${2:collapseChange}\">",
            "\t<bm-collapse-item title=\"${3:title}\" name=\"${4:name}\">",
            "\t\t<div>$5</div>",
            "\t</bm-collapse-item>",
            "</bm-collapse>"
        ]
    },
