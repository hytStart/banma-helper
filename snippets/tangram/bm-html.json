        "body": "<bm-checkbox-button :label=\"$1\" :key=\"$2\" >$3</bm-checkbox-button>"
    },
    "bm-checkbox-group": {
        "prefix": "bm-checkbox-group",
        "body": [
            "<bm-checkbox-group v-model=\"$1\">",
            "\t<bm-checkbox :label=\"${2:label}\" :key=\"${3:key}\">$3</bm-checkbox>",
            "</bm-checkbox-group>"
        ]
