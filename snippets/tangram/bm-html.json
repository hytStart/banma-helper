    "bm-date-picker:datetime": {
        "prefix": "bm-date-picker:datetime",
        "body": "<bm-date-picker v-model=\"$1\" type=\"datetime\" placeholder=\"${2:datetime}\"></bm-date-picker>"
    },
    "bm-dialog": {
        "prefix": "bm-dialog",
        "body": [
            "<bm-dialog",
            "\ttitle=\"\"",
            "\t:visible.sync=\"${1:dialogVisible}\"",
            "\twidth=\"${2:width}\"",
            "\t:before-close=\"${3:dialogBeforeClose}\">",
            "\t<div>$4</div>",
            "\t<div slot=\"footer\">",
            "\t\t<bm-button @click=\"dialogVisible = false\">取 消</bm-button>",
            "\t\t<bm-button type=\"primary\" @click=\"dialogVisible = false\">确 定</bm-button>",
            "\t</div>",
            "</bm-dialog>"
        ]
    },
    "bm-dialog:form": {
        "prefix": "bm-dialog:form",
        "body": [
            "<bm-dialog",
            "\ttitle=\"\"",
            "\t:visible.sync=\"${1:dialogVisible}\"",
            "\twidth=\"${2:width}\"",
            "\t:before-close=\"${3:dialogBeforeClose}\">",
            "\t<bm-form :model=\"form\">",
            "\t\t<bm-form-item label=\"\" :label-width=\"\">",
            "\t\t\t<bm-input v-model=\"\"></bm-input>",
            "\t\t</bm-form-item>",
            "\t</bm-form>",
            "\t<div slot=\"footer\">",
            "\t\t<bm-button @click=\"dialogVisible = false\">取 消</bm-button>",
            "\t\t<bm-button type=\"primary\" @click=\"dialogVisible = false\">确 定</bm-button>",
            "\t</div>",
            "</bm-dialog>"
        ]
    },
    "bm-dropdown": {
        "prefix": "bm-dropdown",
        "body": [
            "<bm-dropdown>",
            "\t<bm-button type=\"primary\">",
            "\t\t下拉菜单<i class=\"bm-icon-arrow-down bm-icon--right\"></i>",
            "\t</bm-button>",
            "\t<bm-dropdown-menu slot=\"dropdown\">",
            "\t\t<bm-dropdown-item>黄金糕</bm-dropdown-item>",
            "\t</bm-dropdown-menu>",
            "</bm-dropdown>"
        ]
    },
    "bm-form": {
        "prefix": "bm-form",
        "body": [
            "<bm-form ref=\"${1:form}\" :model=\"${2:form}\" label-width=\"${3:80px}\">",
            "\t$4",
            "</bm-form>"
        ]
    },
    "bm-form:inline": {
        "prefix": "bm-form:inline",
        "body": [
            "<bm-form :inline=\"true\" ref=\"${1:form}\" :model=\"${2:form}\" label-width=\"${3:80px}\">",
            "\t$4",
            "</bm-form>"
        ]
    },
    "bm-form-item:input": {
        "prefix": "bm-form-item:input",
        "body": [
            "<bm-form-item label=\"${1:label}\">",
            "\t<bm-input v-model=\"${2:model}\"></bm-input>",
            "</bm-form-item>"
        ]
    },
