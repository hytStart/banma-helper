    "bm-form-item:select": {
        "prefix": "bm-form-item:select",
        "body": [
            "<bm-form-item label=\"${1:label}\">",
            "\t<bm-select v-model=\"${2:model}\" placeholder=\"${3:placeholder}\">",
            "\t\t<bm-option label=\"${4:label}\" value=\"${5:value}\"></bm-option>",
            "\t</bm-select>",
            "</bm-form-item>"
        ]
    },
    "bm-form-item:switch": {
        "prefix": "bm-form-item:switch",
        "body": [
            "<bm-form-item label=\"${1:label}\">",
            "<bm-switch v-model=\"${2:model}\"></bm-switch>",
            "</bm-form-item>"
        ]
    },
    "bm-form-item:textarea": {
        "prefix": "bm-form-item:textarea",
        "body": [
            "<bm-form-item label=\"${1:label}\">",
            "<bm-input type=\"textarea\" v-model=\"${2:model}\"></bm-input>",
            "</bm-form-item>"
        ]
    },
    "bm-input": {
        "prefix": "bm-input",
        "body": "<bm-input v-model=\"$1\" placeholder=\"$2\"></bm-input>"
    },
    "bm-input-number": {
        "prefix": "bm-input-number",
        "body": "<bm-input-number v-model=\"$1\" @change=\"${2:inputChange}\" :min=\"${3:1}\" :max=\"${4:10}\" label=\"${5:label}\"></bm-input-number>"
    },
