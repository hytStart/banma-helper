    "bm-upload:avatar": {
        "prefix": "bm-upload:avatar",
        "body": [
            "<bm-upload",
            "\taction=\"\"",
            "\t:show-file-list=\"false\"",
            "\t:on-success=\"\"",
            "\t:before-upload=\"\">",
            "\t<img v-if=\"imageUrl\" :src=\"imageUrl\">",
