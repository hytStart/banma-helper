        "prefix": "bm-input:textarea",
        "body": "<bm-input type=\"textarea\" v-model=\"$1\" placeholder=\"$2\"></bm-input>"
    },
    "bm-menu": {
        "prefix": "bm-menu",
        "body": [
            "<bm-menu :default-active=\"${1:activeIndex}\" mode=\"horizontal\" @select=\"${2:menuSelect}\">",
            "\t<bm-menu-item index=\"${3:index}\">$4</bm-menu-item>",
            "</bm-menu>"
        ]
    },
    "bm-menu:submenu": {
        "prefix": "bm-menu:submenu",
        "body": [
            "<bm-menu :default-active=\"${1:activeIndex}\" mode=\"horizontal\" @select=\"handleSelect\">",
            "\t<bm-menu-item index=\"1\"></bm-menu-item>",
            "\t<bm-submenu index=\"2\">",
            "\t\t<template slot=\"title\"></template>",
            "\t\t<bm-menu-item index=\"2-1\"></bm-menu-item>",
            "\t</bm-submenu>",
            "</bm-menu>"
        ]
    },
    "bm-pagination": {
        "prefix": "bm-pagination",
        "body": [
            "<bm-pagination",
            "\tlayout=\"prev, pager, next\"",
            "\t:total=\"50\">",
            "</bm-pagination>"
        ]
    },
    "bm-pagination:full": {
        "prefix": "bm-pagination:full",
        "body": [
            "<bm-pagination",
            "\t@size-change=\"handleSizeChange\"",
            "\t@current-change=\"handleCurrentChange\"",
            "\t:current-page=\"pageNum\"",
            "\t:page-sizes=\"[10, 20, 50]\"",
            "\t:page-size=\"pageSize\"",
            "\tlayout=\"total, sizes, prev, pager, next, jumper\"",
            "\t:total=\"totalCount\">",
            "</bm-pagination>"
        ]
    },
    "bm-popover": {
        "prefix": "bm-popover",
