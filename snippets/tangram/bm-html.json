        "prefix": "bm-form-item:checkbox",
        "body": [
            "<bm-form-item label=\"${1:label}\">",
            "\t<bm-checkbox-group v-model=\"${2:model}\">",
            "\t\t<bm-checkbox label=\"${3:label}\" name=\"${4:name}\"></bm-checkbox>",
            "\t</bm-checkbox-group>",
            "</bm-form-item>"
